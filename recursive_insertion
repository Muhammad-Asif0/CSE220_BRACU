def bubbleSort(head, i, j):
    if i > 0: # 4> 0
        if j < i:                # 0 < 4
            if arr[j] > arr[j + 1]:
                arr[j], arr[j + 1] = arr[j + 1], arr[j]
            bubbleSort(head, i, j + 1)
        bubbleSort(head, i - 1, 0)

bubbleSort(arr, len(arr) - 1, 0)
#                  4    ,   0
print(arr)
